scenario
{
    name    = "node_fun";
    warmup      = 10;
    duration    = 30;
    cooldown    = 10;
 
    default 
    {
        setheader
        {
            name    = "Connection";
            value   = "keep-alive";
        }
        setheader
        {
            name    = "Host";
            value   = server();
        }
        version     = HTTP11;
        statuscode  = 200;
        close       = ka;
    }
     
    transaction
    {
        id = "foo";
        weight = 100;
        request
        {
            port = 8086;
            url = "/nodetest/non-blocking";
        }
    }
}